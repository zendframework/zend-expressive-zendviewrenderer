{
    "name": "zendframework/zend-expressive-zendviewrenderer",
    "description": "zend-view PhpRenderer integration for Expressive",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "expressive",
        "http",
        "middleware",
        "psr",
        "psr-7",
        "zf2"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "^1.0",
        "zendframework/zend-expressive-helpers": "^1.1 || ^2.0",
        "zendframework/zend-expressive-template": "^1.0.1",
        "zendframework/zend-filter": "^2.5",
        "zendframework/zend-i18n": "^2.5",
        "zendframework/zend-servicemanager": "^2.7.5 || ^3.0.3",
        "zendframework/zend-view": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
      "psr-4": {
        "Zend\\Expressive\\ZendView\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ZendTest\\Expressive\\ZendView\\": "test/"
      }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
