{
    "name": "zendframework/zend-expressive-zendviewrenderer",
    "description": "zend-view PhpRenderer integration for Expressive",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "expressive",
        "http",
        "middleware",
        "psr",
        "psr-7",
        "zf2"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": "^7.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-expressive-template": "^1.0.1",
        "zendframework/zend-filter": "^2.6.1",
        "zendframework/zend-i18n": "^2.6",
        "zendframework/zend-servicemanager": "^3.0.3",
        "zendframework/zend-view": "^2.6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "zendframework/zend-coding-standard": "^1.0",
        "malukenho/docheader": "^0.1.5"
    },
    "autoload": {
      "psr-4": {
        "Zend\\Expressive\\ZendView\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ZendTest\\Expressive\\ZendView\\": "test/"
      }
    },
    "scripts": {
        "check": [
            "@license-check",
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "license-check": "docheader check src/ test/",
        "test": "phpunit --colors=always"
    }
}
